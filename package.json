{
  "name": "simfile-parser",
  "version": "0.5.0",
  "description": "Read stepmania charts with javascript!",
  "type": "module",
  "main": "./dist/main.js",
  "browser": "./dist/browser.js",
  "types": "./dist/main.d.ts",
  "bin": "./dist/cli.js",
  "sideEffects": "false",
  "scripts": {
    "test": "jest",
    "lint": "eslint --max-warnings 0 ./src",
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist build",
    "clean-build": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noahm/simfile-parser.git"
  },
  "keywords": [
    "stepmania",
    "itg",
    "stepfile",
    "ddr"
  ],
  "author": {
    "name": "Noah Manneschmidt",
    "email": "noah@manneschmidt.net"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noahm/simfile-parser/issues"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "homepage": "https://github.com/noahm/simfile-parser#readme",
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.4.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "confusing-browser-globals": "^1.0.11",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.4",
    "eslint-plugin-jsdoc": "^46.2.6",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.0.0",
    "prettier": "^2.5.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.0"
  },
  "resolutions": {
    "semver": "^7.5.2"
  }
}
