{
  "name": "simfile-parser",
  "version": "0.7.2",
  "description": "Read stepmania charts with javascript!",
  "type": "module",
  "main": "./dist/main.js",
  "browser": "./dist/browser/index.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": "./dist/main.js",
    "./browser": "./dist/browser/index.js"
  },
  "bin": "./dist/cli.js",
  "sideEffects": "false",
  "scripts": {
    "test": "jest",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint --max-warnings 0 ./src",
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist build",
    "clean-build": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noahm/simfile-parser.git"
  },
  "keywords": [
    "stepmania",
    "itg",
    "stepfile",
    "ddr"
  ],
  "author": {
    "name": "Noah Manneschmidt",
    "email": "noah@manneschmidt.net"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noahm/simfile-parser/issues"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  },
  "engines": {
    "node": ">=16.9.0"
  },
  "homepage": "https://github.com/noahm/simfile-parser#readme",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^22.7.4",
    "@types/wicg-file-system-access": "^2023.10.1",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "confusing-browser-globals": "^1.0.11",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-jsdoc": "^50.3.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "next",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "semver": "^7.5.2"
  },
  "packageManager": "yarn@4.1.0"
}
